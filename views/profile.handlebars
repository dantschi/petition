<h2>Please tell us just a little bit more about yourself</h2>


<form method="post">
    {{#if notNumber}}
        <p class="error">"Age" must be a number!</p>
    {{/if}}
    <input type="number" name="age" placeholder="Age (not required)" min="0" max="150" autocomplete="off">
    <input type="text" name="city" placeholder="City (not required)" autocomplete="off">
    <input type="text" name="url" placeholder="Homepage (not required)"  autocomplete="off">
    <input type="hidden" name="_csrf" value="{{csrfToken}}">
    <button id="btn-profile" type="submit" name="button">Continue</button>
</form>

{{#if loggedIn}}
    {{>logout}}
{{/if}}
